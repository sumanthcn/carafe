{
  "collectionName": "components_shared_product_schema",
  "info": {
    "displayName": "Product Schema",
    "icon": "code",
    "description": "Product structured data for SEO"
  },
  "options": {},
  "attributes": {
    "brand": {
      "type": "string",
      "default": "Carafe Coffee"
    },
    "gtin": {
      "type": "string"
    },
    "mpn": {
      "type": "string"
    },
    "aggregateRating": {
      "type": "decimal",
      "min": 1,
      "max": 5
    },
    "reviewCount": {
      "type": "integer",
      "default": 0
    },
    "availability": {
      "type": "enumeration",
      "enum": ["InStock", "OutOfStock", "PreOrder", "Discontinued"],
      "default": "InStock"
    }
  }
}
