{
  "collectionName": "components_elements_nav_child",
  "info": {
    "displayName": "Navigation Child",
    "icon": "link",
    "description": "Child/dropdown navigation item"
  },
  "options": {},
  "attributes": {
    "label": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string",
      "required": true,
      "default": "/"
    },
    "linkType": {
      "type": "enumeration",
      "enum": ["internal", "external"],
      "default": "internal"
    },
    "page": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::page.page"
    },
    "isVisible": {
      "type": "boolean",
      "default": true
    },
    "openInNewTab": {
      "type": "boolean",
      "default": false
    }
  }
}
