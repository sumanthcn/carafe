{
  "kind": "component",
  "collectionName": "components_elements_nav_item",
  "info": {
    "displayName": "Navigation Item",
    "icon": "link",
    "description": "Navigation menu item for header/footer"
  },
  "options": {},
  "attributes": {
    "label": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string",
      "required": true,
      "default": "/"
    },
    "linkType": {
      "type": "enumeration",
      "enum": [
        "internal",
        "external"
      ],
      "default": "internal",
      "required": true
    },
    "page": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::page.page"
    },
    "order": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "isVisible": {
      "type": "boolean",
      "default": true
    },
    "openInNewTab": {
      "type": "boolean",
      "default": false
    },
    "children": {
      "type": "component",
      "repeatable": true,
      "component": "elements.nav-child"
    }
  }
}
